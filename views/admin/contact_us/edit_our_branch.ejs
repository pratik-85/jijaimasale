<%- include("../navbar.ejs") %>
<div class="container p-5 bg-white shadow mb-5">
    <div class="row">
        <div class="col-md-12">
            <h1 class="text-center font-weight-bold">CONTACT US PAGE</h1>
        </div>
    </div>
</div>


<form action="/admin/Update_our_branches_save" method="post" enctype="multipart/form-data">
    <div class="container p-5 bg-white shadow mb-5">
        <div class="row">
            <div class="col-md-12 text-center mb-4">
                <h3 class="font-weight-bold">OUR BRANCHES</h3>
            </div>
            <input type="hidden" name="update_branch_id" value="<%=edit_branch_data.our_branches_id%>">
            <div class="col-md-6">
                Enter Shop Place
                <input type="text" value="<%=edit_branch_data.shop_place%>" name="shop_place" class="form-control">
            </div>
            <div class="col-md-6">
                Enter Shop Name
                <input type="text" value="<%=edit_branch_data.shop_name%>" name="shop_name" class="form-control">
            </div>
            <div class="col-md-6 mt-4">
                Enter Mobile No
                <input type="text" value="<%=edit_branch_data.shop_mobile_no%>" name="shop_mobile_no" class="form-control">
            </div>
            <div class="col-md-6 mt-4">
                Enter Email Id
                <input type="email" value="<%=edit_branch_data.shop_email%>" name="shop_email" class="form-control">
            </div>
            <div class="col-md-12 mt-4">
                Enter Address
                <textarea name="shop_address" class="form-control"><%=edit_branch_data.shop_address%></textarea>
            </div>
            <div class="col-md-12 text-center mt-4">
                <button class="btn btn-primary">Save Brach</button>
            </div>
        </div>
    </div>
</form>
<%- include("../footer.ejs") %>
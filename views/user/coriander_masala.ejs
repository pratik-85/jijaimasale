<%- include("navbar.ejs")%>


<div class="container mt-3">
    <div class="row">
        <div class="col-md-12 p-0">
            <h2 class="alert alert-success font-weight-bold p-4 text-center">Coriander Masala</h2>
        </div>
    </div>
</div>


<section>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="mutih1 text-uppercase"><strong></strong></div>
            </div>
        </div>
        <% for(var i=0;i<coriander_masala.length;i++){%>
        <div class="row mt-5">
            <div class="col-lg-12">
                <img src="/images/<%=coriander_masala[i].banner_img%>" class="img-fluid" alt="">
            </div>
        </div>
        <%}%>
        <div class="row">
        <% for(var i=0;i<coriander_masala.length;i++){%>
            <div class="col-lg-4">
                <br><img src="/images/<%=coriander_masala[i].img1%>" class="img-fluid" alt="">
                <br><br><img src="/images/<%=coriander_masala[i].img2%>" class="img-fluid" alt="">
                <br><br>
            </div>
            <%}%>
        <% for(var i=0;i<coriander_masala.length;i++){%>
            <div class="col-lg-8">
                <h2 class="color_red mt-5"><%=coriander_masala[i].heading1%></h2>
                <p class="font-family"><b><%=coriander_masala[i].description1%></b></p>
                <h2 class="color_red"><%=coriander_masala[i].heading2%></h2>
                <% for(var i=0;i<description.length;i++){%>
                    <p class="font-weight-bold mt-3"><%=description[i].description%></p>
                    <%}%>
             <% for(var i=0;i<coriander_masala.length;i++){%>
                <h2 class="color_red"><%=coriander_masala[i].heading3%></h2>
                <%}%>
                <p style="font-size:16px"><b>
                        <ul>
                            <% for(var i=0;i<specificationdata.length;i++){%>
                                <li>
                                    <p class="font-family"><%=specificationdata[i].specification%></p>
                                </li>
                            <%}%>
                        </ul>
                    </b></p>
             <% for(var i=0;i<coriander_masala.length;i++){%>
                <h2 class="color_red"><%=coriander_masala[i].heading4%></h2>
                <%}%>
                <div class="d-flex">
                    <% for(var i=0;i<quantitydata.length;i++){%>
                        <span class="ml-4 font-weight-bold">
                            <%=quantitydata[i].quantity%>
                        </span>
                        <%}%>
                </div>
        <% for(var i=0;i<coriander_masala.length;i++){%>
                <h2 class="color_red"><%=coriander_masala[i].heading5%></h2>
                <%}%>
                </p>
            </div>
        <%}%>
        </div>
    </div>
</section>

<% for(var i=0;i<coriander_masala.length;i++){%>
<div class="container mb-4">
    <div class="row">
        <div class="col-md-5 mt-2">
            <img src="/images/<%=coriander_masala[i].img3%>" alt="First Image" class="img-fluid">
        </div>
        <div class="col-md-7 mt-2">
            <img src="/images/<%=coriander_masala[i].img4%>" alt="Second Image" class="img-fluid h-100">
        </div>
    </div>
</div>
<%}%>


<%- include("footer.ejs")%>